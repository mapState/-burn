<view class="main"><image class="bgImg" src="../../static/img/fua.png" mode="widthFix"></image><view class="top"><image class="borderImg" src="../../static/img/border.png" mode="aspectFill"></image><text class="title">本命星盘 福根无穷</text></view><view class="want"><view class="wantText">心中所求:</view></view><block wx:if="{{wishList.length>0}}"><view class="wantList"><block wx:for="{{wishList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><text class="wantListItem">{{item}}</text></block></view></block><block wx:if="{{step===1}}"><view class="selBox"><view class="selBoxTxt">生辰八字:</view></view><view data-event-opts="{{[['tap',[['showDateDialog',[2,'number']]]]]}}" class="calendarBox" catchtap="__e"><input class="dateInput" type="text" value="{{endDate}}" placeholder-class="plClass" disabled="{{true}}" placeholder="请选择您的生辰" data-event-opts="{{[['input',[['__set_model',['','endDate','$event',[]]]]]]}}" bindinput="__e"/></view></block><block wx:if="{{step===2}}"><view class="want"><view class="wantText">性别:</view></view><block wx:for="{{sex}}" wx:for-item="item" wx:for-index="index" wx:key="index"><text data-event-opts="{{[['tap',[['selSex',[index]]]]]}}" class="{{['sexItem',(index==sexIndex)?'sexActive':'']}}" bindtap="__e">{{item}}</text></block></block><block wx:if="{{step===3}}"><view class="want"><view class="wantText">出生方位：</view></view><view class="calendarBox" style="margin-bottom:60rpx;"><pick-regions vue-id="2fd7e2a0-1" defaultRegion="{{defaultRegionCode}}" data-event-opts="{{[['^getRegion',[['handleGetRegion']]]]}}" bind:getRegion="__e" bind:__l="__l" vue-slots="{{['default']}}"><input class="dateInput" type="text" value="{{regionName}}" placeholder-class="plClass" disabled="{{true}}" placeholder="请选择出生方位" data-event-opts="{{[['input',[['__set_model',['','regionName','$event',[]]]]]]}}" bindinput="__e"/></pick-regions></view><view class="want"><view class="wantText">现居方位：</view></view><view class="calendarBox" style="margin-bottom:60rpx;"><pick-regions vue-id="2fd7e2a0-2" defaultRegion="{{defaultRegionCode}}" data-event-opts="{{[['^getRegion',[['handleGetRegion1']]]]}}" bind:getRegion="__e" bind:__l="__l" vue-slots="{{['default']}}"><input class="dateInput" type="text" value="{{regionName1}}" placeholder-class="plClass" disabled="{{true}}" placeholder="请选择现居方位" data-event-opts="{{[['input',[['__set_model',['','regionName1','$event',[]]]]]]}}" bindinput="__e"/></pick-regions></view></block><block wx:if="{{step===4}}"><view class="nameBox" style="margin-top:60rpx;"><text class="nameTitle">姓氏：</text><input class="nameInput" type="text" value="{{nameForom.surname}}" data-event-opts="{{[['input',[['__set_model',['$0','surname','$event',[]],['nameForom']]]]]}}" bindinput="__e"/></view><view class="nameBox"><text class="nameTitle">名讳：</text><input class="nameInput" type="text" value="{{nameForom.name}}" data-event-opts="{{[['input',[['__set_model',['$0','name','$event',[]],['nameForom']]]]]}}" bindinput="__e"/></view></block><block wx:else><view data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="btnBox" bindtap="__e"><image class="btnImg" src="../../static/img/btn.png" mode="aspectFill"></image><text class="btnText">确定</text></view></block><block wx:if="{{step==4}}"><view class="btnBox"><image class="btnImg" src="../../static/img/btn.png" mode="aspectFill"></image><text data-event-opts="{{[['tap',[['goMatchDojo',['$event']]]]]}}" class="btnText" bindtap="__e">开始匹配本命财神道场</text></view></block><zan-calendar vue-id="2fd7e2a0-3" date="{{date}}" time="{{time}}" isHourShow="{{isHourShow}}" isMinShow="{{isMinShow}}" show="{{dateDialog}}" lunarType="{{lunarType}}" data-event-opts="{{[['^closeDialog',[['closeDialog']]],['^confirmDialog',[['confirmDialog']]],['^selType',[['getType']]]]}}" bind:closeDialog="__e" bind:confirmDialog="__e" bind:selType="__e" bind:__l="__l"></zan-calendar></view>