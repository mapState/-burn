<view class="main"><view class="leftIconBox" style="{{'top:'+(top+'px')+';'}}"><image class="leftIcon" src="../../static/img/left.png" mode="aspectFill" data-event-opts="{{[['tap',[['goBack',['$event']]]]]}}" bindtap="__e"></image></view><image class="topImg" src="../../static/tmp/x1.png" mode="widthFix"></image><view class="detail"><view class="dTitle"><text class="dTitleTxt">{{detail.info||detail.name}}</text></view><view class="eightBox"><text class="t1">匹配本命财神道场</text><text class="t1">福生无量天尊法无边</text><image class="egImg" src="../../static/tmp/eg.png" mode="aspectFill"></image></view><view class="t2"><rich-text nodes="{{detail.content}}"></rich-text></view><image class="godImg" src="../../static/tmp/gossip.png" mode="aspectFill"></image><block wx:if="{{showBtn}}"><view class="btnBox"><image class="btnImg" src="../../static/img/btn.png" mode="aspectFill"></image><view data-event-opts="{{[['tap',[['open',['$event']]]]]}}" class="btnText" bindtap="__e">立即供奉</view></view></block></view><uni-popup class="vue-ref" vue-id="1" type="center" maskClick="{{false}}" data-ref="popup" bind:__l="__l" vue-slots="{{['default']}}"><view class="countdown"><view class="countdownTop"><image class="ylIcon" src="../../static/img/yl.png" mode="aspectFill"></image><text class="countdownTopText">供奉倒计时</text></view><view class="time"><text class="timeNum">{{"0"+time}}</text>s</view><view data-event-opts="{{[['tap',[['close',['$event']]]]]}}" class="closeBox" bindtap="__e"><image class="closeIcon" src="../../static/img/closeIcon.png" mode="aspectFill"></image><view class="line"></view></view></view></uni-popup></view>