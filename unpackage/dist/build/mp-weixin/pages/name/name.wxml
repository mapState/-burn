<view class="main"><image class="bgImg" src="../../static/img/fua.png" mode="widthFix"></image><view class="top"><image class="borderImg" src="../../static/img/border.png" mode="aspectFill"></image><text class="title">本命星盘  福报无穷</text></view><view class="want"><view class="wantText">心中所求:</view></view><block wx:if="{{wishList.length>0}}"><view class="wantList"><block wx:for="{{wishList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><text class="wantListItem">{{item}}</text></block></view></block><view class="nameBox" style="margin-top:60rpx;"><text class="nameTitle">姓氏：</text><input class="nameInput" type="text" value="{{nameForom.surname}}" data-event-opts="{{[['input',[['__set_model',['$0','surname','$event',[]],['nameForom']]]]]}}" bindinput="__e"/></view><view class="nameBox"><text class="nameTitle">名讳：</text><input class="nameInput" type="text" value="{{nameForom.name}}" data-event-opts="{{[['input',[['__set_model',['$0','name','$event',[]],['nameForom']]]]]}}" bindinput="__e"/></view><view class="btnBox"><image class="btnImg" src="../../static/img/btn.png" mode="aspectFill"></image><text data-event-opts="{{[['tap',[['goMatchDojo',['$event']]]]]}}" class="btnText" bindtap="__e">开始匹配本命财神道场</text><block wx:if="{{!hasPhone}}"><button class="getPhoneBtn" open-type="getPhoneNumber" data-event-opts="{{[['getphonenumber',[['getPhoneNumber',['$event']]]]]}}" bindgetphonenumber="__e"></button></block></view><zan-calendar vue-id="28f7d9e0-1" date="{{date}}" time="{{time}}" isHourShow="{{isHourShow}}" isMinShow="{{isMinShow}}" show="{{dateDialog}}" lunarType="{{lunarType}}" data-event-opts="{{[['^closeDialog',[['closeDialog']]],['^confirmDialog',[['confirmDialog']]],['^selType',[['getType']]]]}}" bind:closeDialog="__e" bind:confirmDialog="__e" bind:selType="__e" bind:__l="__l"></zan-calendar></view>